{
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "postcss-cli": "^9.1.0"
  },
  "scripts": {
    "style": "sass --watch network/static/network/custom.scss network/css/stylespre.css",
    "style-postcss": "postcss network/css/stylespre.css -u autoprefixer -o network/static/network/styles.css --watch",
    "style-min": "sass --watch network/static/network/custom.scss --style compressed network/css/stylespremin.css",
    "style-min-postcss": "postcss network/css/stylespremin.css -u autoprefixer -o network/static/network/stylesmin.css --watch",
    "compile-all-styles": "npm-run-all --parallel style style-min style-postcss style-min-postcss"
  }
}
